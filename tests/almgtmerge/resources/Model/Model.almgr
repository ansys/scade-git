  <traceableElements id="_2TRfMvWeEe6JHos9p48C6A" name="CC_HLR_IN_07" identifier="CC_HLR_IN_07" description="Accel: Accelerator pedal sensor&#xA;"/>
  <traceableElements id="_2TRfM_WeEe6JHos9p48C6A" name="CC_HLR_IN_08" identifier="CC_HLR_IN_08" description="Brake: Brake pedal sensor&#xA;"/>
  <traceableElements id="_2TRfNPWeEe6JHos9p48C6A" name="CC_HLR_IN_09" identifier="CC_HLR_IN_09" description="Speed: Car speed sensor"/>
  <traceableElements id="_2TRfNfWeEe6JHos9p48C6A" name="CC_HLR_OUT_01" identifier="CC_HLR_OUT_01" description="CruiseSpeed: Cruise speed value"/>
  <traceableElements id="_2TRfNvWeEe6JHos9p48C6A" name="CC_HLR_OUT_02" identifier="CC_HLR_OUT_02" description="ThrottleCmd: Throttle command"/>
  <traceableElements id="_2TRfN_WeEe6JHos9p48C6A" name="CC_HLR_OUT_03" identifier="CC_HLR_OUT_03" description="CruiseState: this is the state of the CC. It can be OFF, ON, STDBY, INT.&#xA;"/>
  <traceableElements id="_2TRfOPWeEe6JHos9p48C6A" name="CC_HLR_CCB_01" identifier="CC_HLR_CCB_01" description="When the driver starts the car, the CC shall be off.&#xA;The output CruiseState should be set to OFF"/>
  <traceableElements id="_2TRfOfWeEe6JHos9p48C6A" name="CC_HLR_CCB_02" identifier="CC_HLR_CCB_02" description="The CC shall be set on when the driver push the on button."/>
  <traceableElements id="_2TRfOvWeEe6JHos9p48C6A" name="CC_HLR_CCB_03" identifier="CC_HLR_CCB_03" description="The CC shall automatically go off when the off button is pressed."/>
  <traceableElements id="_2TRfO_WeEe6JHos9p48C6A" name="CC_HLR_CCB_04" identifier="CC_HLR_CCB_04" description="If the car speed is in the speed limit and the accelerator pedal is not pressed, the CC shall be on and regulate the car speed.&#xA;The output CruiseState shall be set to ON."/>
  <traceableElements id="_2TRfPPWeEe6JHos9p48C6A" name="CC_HLR_CCB_05" identifier="CC_HLR_CCB_05" description="The CC system shall be automatically disabled when the accelerator pedal is pressed, or the car speed is outside the speed limit.&#xA;The output CruiseState shall be set to STDBY."/>
  <traceableElements id="_2TRfPfWeEe6JHos9p48C6A" name="CC_HLR_CCB_06" identifier="CC_HLR_CCB_06" description="The system shall return to the ON state when both the accelerator pedal is not pressed, and the car speed is inside the speed limit.&#xA;The last set cruise speed shall be re-used.&#xA;"/>
  <traceableElements id="_2TRfPvWeEe6JHos9p48C6A" name="CC_HLR_CCB_07" identifier="CC_HLR_CCB_07" description="The CC shall be immediately interrupted when the brake is pressed.&#xA;The output CruiseState shall be set to INT."/>
  <traceableElements id="_2TRfP_WeEe6JHos9p48C6A" name="CC_HLR_CCB_08" identifier="CC_HLR_CCB_08" description="The system shall resume either to the ON or STDBY states, depending on the Accelerator pedal and the Speed value when the resume button is pressed. The last set cruise speed shall be re-used."/>
  <traceableElements id="_2TRfQPWeEe6JHos9p48C6A" name="CC_HLR_CDC_01" identifier="CC_HLR_CDC_01" description="When the CC is off, the car speed shall be driven using the accelerator pedal."/>
  <traceableElements id="_2TRfQfWeEe6JHos9p48C6A" name="CC_HLR_CDC_02" identifier="CC_HLR_CDC_02" description="When the CC is on, the car speed shall be automatically regulated."/>
  <traceableElements id="_2TRfQvWeEe6JHos9p48C6A" name="CC_HLR_CDC_03" identifier="CC_HLR_CDC_03" description="The regulation shall be done using a proportional and integral algorithm, with Kp and Ki factors."/>
  <traceableElements id="_2TRfQ_WeEe6JHos9p48C6A" name="CC_HLR_CDC_04" identifier="CC_HLR_CDC_04" description="The regulation algorithm shall be protected against the overshoot of its integral part: the integral action shall be reset when the CC is going on, and frozen when the throttle output is saturated."/>
  <traceableElements id="_2TRfRPWeEe6JHos9p48C6A" name="CC_HLR_CDC_05" identifier="CC_HLR_CDC_05" description="The throttle command shall be saturated at ThrottleSatMax when automatically regulating, in order to limit the car acceleration for the comfort."/>
  <traceableElements id="_2TRfRfWeEe6JHos9p48C6A" name="CC_HLR_CSM_01" identifier="CC_HLR_CSM_01" description="The Cruise Speed shall be managed only when the cruise control is enabled, meaning ON, STDBY, or INT states."/>
  <traceableElements id="_2TRfRvWeEe6JHos9p48C6A" name="&lt;Missing Requirement ID>_1" identifier="&lt;Missing Requirement ID>_1" description=""/>
  <traceableElements id="_2TRfR_WeEe6JHos9p48C6A" name="CC_HLR_CSM_02" identifier="CC_HLR_CSM_02" description="The cruise speed shall be set to the current speed when the set button is pressed."/>
  <traceableElements id="_2TRfSPWeEe6JHos9p48C6A" name="CC_HLR_CSM_03" identifier="CC_HLR_CSM_03" description="The cruise speed shall be increased of SpeedInc km/h when the QUICKACCEL button is pressed.&#xA;only if this new value of the cruise speed is still lower than the maximal speed SpeedMax km/h."/>
  <traceableElements id="_2TRfSfWeEe6JHos9p48C6A" name="CC_HLR_CSM_04" identifier="CC_HLR_CSM_04" description="The cruise speed shall be decreased of SpeedInc km/h when the QUICKDECEL button is pressed."/>
  <traceableElements id="_2TRfSvWeEe6JHos9p48C6A" name="CC_HLR_CSM_05" identifier="CC_HLR_CSM_05" description="The Cruise Speed shall be maintained between SpeedMin and SpeedMax km/h values."/>
  <traceableElements id="_2TRfS_WeEe6JHos9p48C6A" name="CC_HLR_PPD_01" identifier="CC_HLR_PPD_01" description="The accelerator pedal shall be detected as pressed when its value is above PedalsMin.&#xA;"/>
  <traceableElements id="_2TRfTPWeEe6JHos9p48C6A" name="CC_HLR_PPD_02" identifier="CC_HLR_PPD_02" description="The Brake pedal shall be detected as pressed when its value is above PedalsMin."/>
  <traceableElements id="_2TRfTfWeEe6JHos9p48C6A" name="CC_HLR_CCP_01" identifier="CC_HLR_CCP_01" description="SpeedMin: 30.0 Km/h"/>
  <traceableElements id="_2TRfTvWeEe6JHos9p48C6A" name="CC_HLR_CCP_02" identifier="CC_HLR_CCP_02" description="SpeedMax: 150.0 Km/h&#xA;"/>
  <traceableElements id="_2TRfT_WeEe6JHos9p48C6A" name="CC_HLR_CCP_03" identifier="CC_HLR_CCP_03" description="SpeedInc: 2.5 Km/h"/>
